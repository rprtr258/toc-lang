{
  "name": "toc-lang",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://raymyers.github.io/toc-lang",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.json && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,vue,json}' --ignore-pattern src/generated/",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts,vue,json}'  --ignore-pattern src/generated/",
    "format": "prettier --write 'src/**/*.{js,ts,vue,css,md,json}' --config ./.prettierrc.json",
    "test": "NODE_ENV=test vitest run",
    "coverage": "NODE_ENV=test vitest run --coverage",
    "gen": "lezer-generator src/assets/grammars/toc-lang.lezer -o src/generated/tocLangLezerParser.js"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.4",
    "@codemirror/language": "^6.10.3",
    "@lezer/lr": "^1.4.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/d3": "^7.4.3",
    "browser-fs-access": "^0.35.0",
    "canvg": "^4.0.1",
    "d3": "7.8.5",
    "dagre-d3-es": "7.0.10",
    "peggy": "^3.0.2",
    "vue": "^3.4.0",
    "vue-codemirror": "6.1.1"
  },
  "devDependencies": {
    "@lezer/generator": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "approvals": "^7.0.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.1",
    "vue-eslint-parser": "^9.4.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "3.2.5",
    "typescript": "^5.3.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.8.22"
  }
}
