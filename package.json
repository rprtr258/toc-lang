{
  "name": "toc-lang",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://raymyers.github.io/toc-lang",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -p tsconfig.json && vite build",
    "preview": "vite preview",
    "lint": "eslint '**/*.{ts,vue,json}'",
    "lint:fix": "eslint --fix '**/*.{ts,vue,json}'",
    "format": "prettier --write 'src/**/*.{js,ts,vue,css,md,json}' --config ./.prettierrc.json",
    "ci": "bun lint && bun test && vue-tsc -p tsconfig.json",
    "gen": "lezer-generator src/assets/grammars/toc-lang.lezer -o src/generated/tocLangLezerParser.js"
  },
  "dependencies": {
    "@codemirror/autocomplete": "6.x",
    "@codemirror/language": "6.x",
    "@codemirror/lint": "6.x",
    "@dagrejs/dagre": "latest",
    "@lezer/lr": "^1.4.2",
    "browser-fs-access": "^0.35.0",
    "curved-arrows": "^0.3.0",
    "jiti": "^2.6.1",
    "svg-pan-zoom": "^3.6.1",
    "vue": "^3.4.0",
    "vue-codemirror": "latest"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@eslint/js": "^9.39.2",
    "@lezer/generator": "^1.7.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^9.39.2",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "prettier": "3.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-eslint-parser": "^9.4.0",
    "vue-tsc": "^1.8.22"
  }
}
